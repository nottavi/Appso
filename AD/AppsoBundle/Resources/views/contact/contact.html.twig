{% extends "ADAppsoBundle::pagelayout.html.twig" %}

{% block content %}
<section class="content wrap cf">
        <h1>Contact</h1>
            {% set messages = app.session.flashbag.get('success') %}
            {% if messages|length > 0 %}
            <div class="flash-notice">
            {% for flashMessage in messages %}
                
                <p> {{ flashMessage }}</p>
               
            {% endfor %}
              </div>
            {% endif %}
            {{ form(form) }}
        </div>
    </div>
</section>
{% endblock %}
