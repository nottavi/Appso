<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{% block title %}Recommandations APPSO+{% endblock %}</title>
    <meta name="description" content="Accueil Pharmaceutique des Patients Sans Ordonnance">


    {% include 'ADAppsoBundle::page_head_style.html.twig' %}
    {% include 'ADAppsoBundle::page_head_script.html.twig' %}


    <script src="/bundles/adappso/js/vendor/modernizr-2.6.2.min.js"></script>
</head>



<body>
<div class="page cf">
    <header id="header" class="wrap cf">
        <h1 class="logo">
            <a href="{{ path( ezpublish.rootLocation ) }}" title="" class="logo"><img src="{{ asset('bundles/adappso/images/logo-appso.png') }}" alt="Recommandation APPSO+" /></a>
        </h1>

        <div class="second-logo">
            <a href="#_"><img src="{{ asset('bundles/adappso/images/logo-2.png') }}" alt="Logo secondaire"></a>
        </div>

        <div id="font-size">A-/+</div>

        <div class="header-search">
            <form method="get" action="{{ path( 'ez_legacy', {'module_uri': '/content/search'} ) }}">
                <div>
                    <label for="site-wide-search-field" class="infield">Recherche</label>
                    <input class="search-query span3" type="text" name="SearchText" id="site-wide-search-field" />
                </div>

                <input type="submit" value="Rechercher">
            </form>
        </div>
    </header>
    {% if location is defined %}
        {% set currentPath = location.pathString %}
    {% else %}
        {% set currentPath = "/1/2/" %}
    {% endif %}
    <nav id="main-nav" class="wrap">
        {% block topMenu %}
            {{ render( controller( "ADAppsoBundle:Appso:topMenu", { 'current': currentPath } ) ) }}
        {% endblock %}
    </nav>


    {% if currentPath != "/1/2/" %}{{ render( controller( "ADAppsoBundle:Appso:breadcrumb", { 'pathString': currentPath } ) ) }}{% endif %}

    {% block content %}
        {% if module_result is defined %}<section class="content wrap">{{ module_result.content|raw }}</section>{% endif %}
    {% endblock %}


</div>

<footer id="footer">
    <div class="wrap">
        <div class="footer-nav">
            <div class="cf">
                <a href={{ url('contact') }}>Contact</a>

                <ul>
                    <li><a href="#_">Mentions légales</a></li>
                    <li><a href="#_">Plan du site</a></li>
                    <li><a href="#_">Glossaire</a></li>
                </ul>
            </div>
        </div>

        <div class="partners">
            <ul class="cf">
                <li>
                    <a href="#_">
                                <span class="img">
                                    <img src="img/partners/eqo.png" alt="eQo">
                                </span>
                        <span>Évaluation Qualité Officine</span>
                    </a>
                </li>
                <li>
                    <a href="#_">
                                <span class="img">
                                    <img src="img/partners/meddispar.png" alt="Meddispar Ordre national des pharmaciens">
                                </span>
                        <span>Médicaments à dispensation particulière</span>
                    </a>
                </li>
                <li>
                    <a href="#_">
                                <span class="img">
                                    <img src="img/partners/cespharm.png" alt="Cespharm+ Éducation et prévention pour la santé">
                                </span>
                        <span>Éducation sanitaire</span>
                    </a>
                </li>
                <li>
                    <a href="#_">
                                <span class="img">
                                    <img src="img/partners/pharmavigilance.png" alt="Pharmavigilance">
                                </span>
                        <span>Pharmavigilance</span>
                    </a>
                </li>
            </ul>
        </div>
    </div>
</footer>



<script src="/bundles/adappso/js/vendor/jquery-1.10.2.min.js"></script>
<script src="/bundles/adappso/js/jquery.carouFredSel-6.2.1-packed.js"></script>
<script src="/bundles/adappso/js/jquery.fancybox.pack.js"></script>
<script src="/bundles/adappso/js/jquery.infieldlabel.min.js"></script>
<script src="/bundles/adappso/js/main.js"></script>

